{
  "name": "receipt-automation",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil App Starter",
  "scripts": {
    "build:prod:www": "stencil build --serve",
    "start": "concurrently \"npm:start:proxy\" \"npm:start:dev\"",
    "start:dev": "stencil build --dev --watch --serve",
    "start:prod": "stencil build --watch --serve",
    "test": "stencil test --spec --e2e",
    "unit-test": "stencil test --spec",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "start:proxy": "node proxy.js"
  },
  "devDependencies": {
    "@stencil/core": "^2.0.0",
    "@stencil/router": "^1.0.1",
    "@stencil/sass": "^1.4.1",
    "@types/jest": "^26.0.15",
    "@types/w3c-image-capture": "^1.0.2",
    "express": "^4.17.1",
    "concurrently": "^6.2.0",
    "fs": "0.0.1-security",
    "http-proxy-middleware": "^2.0.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "workbox-build": "^4.3.1"
  },
  "license": "MIT",
  "dependencies": {
    "@types/lodash": "^4.14.168",
    "deepmerge": "^4.2.2",
    "feather-icons": "^4.28.0",
    "flyd": "^0.2.8",
    "immer": "^8.0.0",
    "pixi-viewport": "^4.32.0",
    "pixi.js": "^6.0.0",
    "qr-scanner": "^1.2.0",
    "qrcode": "^1.4.4",
    "reflect-metadata": "^0.1.13",
    "uuidv4": "^6.2.6"
  }
}
